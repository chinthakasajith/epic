package com.epic.cms.admin.controlpanel.sysusermgt.bean;

/**
 * this class use to store information about user session validation 
 * @author ayesh
 */
public class SessionValidBean {

    private String sessionID;
    private String status;

    /**
     * get user session from database
     * @return -String
     */
    public String getSessionID() {
        return sessionID;
    }

    /**
     * set user session id from database
     * @param sessionID 
     */
    public void setSessionID(String sessionID) {
        this.sessionID = sessionID;
    }

    /**
     * get user status form database
     * @return  -String
     */
    public String getStatus() {
        return status;
    }

    /**
     * set status from database
     * @param status -String
     */
    public void setStatus(String status) {
        this.status = status;
    }
}
