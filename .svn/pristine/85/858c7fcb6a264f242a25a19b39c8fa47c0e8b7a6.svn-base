package com.epic.cms.admin.controlpanel.systemconfigmgt.bean;

/**
 *this bean class use to wrote all the bean that relate to CreditScoreRecommandBean
 * @author ayesh
 */
public class CreditScoreRecommandBean {

    private String id;
    private String lowLimit;
    private String highLimit;
    private String cardtype;
    private String cardproduct;
    private String cardproductCode;
    private String creditlimit;
    private String lastUpdateUser;
    private String cardTypeCode;

    public String getCardproduct() {
        return cardproduct;
    }

    public void setCardproduct(String cardproduct) {
        this.cardproduct = cardproduct;
    }

    public String getCardtype() {
        return cardtype;
    }

    public void setCardtype(String cardtype) {
        this.cardtype = cardtype;
    }

    public String getCreditlimit() {
        return creditlimit;
    }

    public void setCreditlimit(String creditlimit) {
        this.creditlimit = creditlimit;
    }

    public String getHighLimit() {
        return highLimit;
    }

    public void setHighLimit(String highLimit) {
        this.highLimit = highLimit;
    }

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public String getLastUpdateUser() {
        return lastUpdateUser;
    }

    public void setLastUpdateUser(String lastUpdateUser) {
        this.lastUpdateUser = lastUpdateUser;
    }

    public String getLowLimit() {
        return lowLimit;
    }

    public void setLowLimit(String lowLimit) {
        this.lowLimit = lowLimit;
    }

    public String getCardTypeCode() {
        return cardTypeCode;
    }

    public void setCardTypeCode(String cardTypeCode) {
        this.cardTypeCode = cardTypeCode;
    }

    public String getCardproductCode() {
        return cardproductCode;
    }

    public void setCardproductCode(String cardproductCode) {
        this.cardproductCode = cardproductCode;
    }
}
