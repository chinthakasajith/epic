function loadColorPalet(){
$('#colorDiv').load('DIE/colorPalet.html');
}


function applicationTableView(url){

$('#previewDiv').load(url);
}





function showExploreResult(){
var st = $('#statusID').val();


switch(st)
{
  case '0':
  applicationTableView('DIE/new.html');
  break;
  case '1':
  applicationTableView('DIE/forwaded.html');
  break;
  case '2':
   applicationTableView('DIE/processed.html');
  break;
  case '3':
   applicationTableView('CIO/cleared_ETC.html');
  break;
  case '4':
  applicationTableView('CIO/closed.html');
  break;
  case '5':
   applicationTableView('SDM/IncompleteData.html');
  break;
  case '6':
  applicationTableView('SDM/pendingOriginals.html');
 
  break;
  case '909':
  applicationTableView('DIE/all_status.html');
  
  break;
 // default:
 // alert('case default 7');
  }

}



</script>



