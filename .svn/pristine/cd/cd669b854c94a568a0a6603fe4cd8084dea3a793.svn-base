/* standard elements */

* {
	margin: 0;
	padding: 0;
}

body {
	background: url(../images/topreapeat2.png) repeat-x;
	font: normal 80% "Lucida Sans Unicode",sans-serif;
	margin: 0;
       
}
/*input {
	font: normal 1.1em "Lucida Sans Unicode",sans-serif;
}*/
input2 {
	font-family: "Lucida Grande", Verdana, Arial, "Bitstream Vera Sans", sans-serif;
	padding: 3px 10px;
	border: none;
	font-size: 12px;
	border-width: 1px;
	border-style: solid;
	-moz-border-radius: 11px;
	-khtml-border-radius: 11px;
	-webkit-border-radius: 11px;
	border-radius: 11px;
	cursor: pointer;
	text-decoration: none;
	margin-top: -3px;
	text-shadow: rgba(0,0,0,0.3) 0 -1px 0;
}
p,cite,code,ul {
	font-size: 105%;
	padding-bottom: 1.2em;
}
h1 {
	font-size: 1.4em;
	margin-bottom: 4px;
}
code {
	background: url(../img/bgcode.gif);
	border: 1px solid #F0F0F0;
	border-left: 6px solid #39F;
	display: block;
	font: normal 1.1em "Lucida Sans Unicode",serif;
	margin-bottom: 12px;
	padding: 8px 10px;
	white-space: pre;
}
cite {
	background: url(../img/quote.gif) no-repeat;
	display: block;
	font: normal 1.3em "Lucida Sans Unicode",serif;
	padding-left: 28px;
}
h1,h2,h3 {
	padding-top: 6px;
}
/* misc */
.clearer {
	clear: both;
}

/* structure */
.container {
	background: url(../images/topbanner.jpg) no-repeat;
        background-size: contain;
}

.header {
	height: 92px;
}

/* title */
.title {
	float: left;
	padding: 28px 0 0 76px;
}
.title h1 {
	font: normal 2em Verdana,sans-serif;
}

.title2 {
	font-family	: Verdana, Arial, Helvetica, sans-serif;
	font-size	: 11px;
	font-weight	: bold;
	color		: #613027;
}
.tit {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 12px;
	font-style: normal;
	font-variant: normal;
	color:#000000;/*themecolour1;*/
	margin-left: 0px;
	font-weight: bold;
	text-transform: uppercase;
}

/* navigation */
.navigation {
	float: left;
	height: 92px;
	margin-left: 24px;
	padding: 0 16px;
}
.navigation a {
	float: left;
	font: bold 1.2em "Trebuchet MS",sans-serif;
	margin-top: 56px;
	padding: 8px 18px;
	text-align: center;
	text-decoration: none;
}
.navigation a:hover {
}

/* main */
.main {
	//background: url(../img/bgmain.gif) no-repeat;
        //background: #FFFFFF;
	clear: both;
	padding: 12px 12px 0 10px;
        overflow: auto;
        width: 98%;
}

/* main left */
.sidenav {
	float: right;
	margin-right: 1%;
	width:auto;
}

.sidenav h1 {
	border-bottom: 1px dashed #DDD;
	font-size: 1.2em;
	height: 20px;
	margin-top: 1.2em;
}
.sidenav ul {
	margin: 0;
	padding: 0;
}
.sidenav li { 
	border-bottom: 1px dashed #EEE;
	list-style: none;
	margin: 0;
}
.sidenav li a {
	display: block;
	font-size: 0.9em;
	padding: 3px 6px 3px 18px;
	text-decoration: none;
}
.sidenav li a:hover {
	background: url(../img/nav_li.gif) no-repeat;
}

/* content */
.content {
	float: left;
	width: 18%;
        min-height: 600px;
	margin:0px 5px 0px 0px;
        
}
.contentrow1 {
	float: left;
	width: 18%;
        min-height: 600px;
	margin:0px 5px 0px 0px;
        overflow: visible;
        z-index: 200;
        
}
.contentrow2 {
	float: left;
	width: 18%;
        min-height: 400px;
	overflow: visible;
	margin:0px 5px 0px 0px;
        z-index: 100;
        
}
.content .descr {;
	margin-bottom: 6px;
}
.content li {
	list-style: url(../img/li.gif);
	margin-left: 18px;
}

/* search form */
.styled {
	border: 3px double #E5E5E5;
	padding: 2px 3px;
}
.button {
	border: 1px solid #AAA;
	margin-left: 5px;
	padding: 2px 3px;
}

/* footer */
.footer {
	background: #000000 url(../images/bgfooter.gif) repeat-x;
	font-size: 0.8em;
        line-height: 40px;
	text-align: center;
        color:#F0F0F0;
}
.footer a {
	text-decoration: none;
	color:#999999
}
.footer a:link  {
	text-decoration: none;
	color:#999999
}
.footer a:visited   {
	text-decoration: none;
	color:#999999
}
.footer a:active   {
	text-decoration: none;
	color:#999999
}
.footer a:hover {
	text-decoration: underline;
	color:#999999
}
.defbutton {
/*    border: 1px solid #AAA;
	margin-left: 5px;
	padding: 2px 3px;*/
    width: 100px;
}
