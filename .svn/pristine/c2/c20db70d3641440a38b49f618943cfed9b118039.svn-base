package com.epic.cms.camm.applicationconfirmation.bean;

/**
 *
 * @author badrika
 */
public class ConfirmBean {
    
    private String cdProduct;
    private String proMode;
    private String binProf;
    private String accTemplt;
    private String cdTemplt;
    private String loyalty;
    private String rejReason;

    public String getLoyalty() {
        return loyalty;
    }

    public void setLoyalty(String loyalty) {
        this.loyalty = loyalty;
    }

    public String getAccTemplt() {
        return accTemplt;
    }

    public void setAccTemplt(String accTemplt) {
        this.accTemplt = accTemplt;
    }

    public String getBinProf() {
        return binProf;
    }

    public void setBinProf(String binProf) {
        this.binProf = binProf;
    }

    public String getCdProduct() {
        return cdProduct;
    }

    public void setCdProduct(String cdProduct) {
        this.cdProduct = cdProduct;
    }

    public String getCdTemplt() {
        return cdTemplt;
    }

    public void setCdTemplt(String cdTemplt) {
        this.cdTemplt = cdTemplt;
    }

    public String getProMode() {
        return proMode;
    }

    public void setProMode(String proMode) {
        this.proMode = proMode;
    }

    public String getRejReason() {
        return rejReason;
    }

    public void setRejReason(String rejReason) {
        this.rejReason = rejReason;
    }
    
    
    
    
}
